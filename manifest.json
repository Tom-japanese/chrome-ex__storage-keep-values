{
  "name": "storage keep values",
  "description": "chrome.storageに値を複数保存する",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "activeTab",
    "tabs"

  ],
  "browser_action": {
    "default_title": "",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["jquery-2.2.0.min.js","script.js"]
  }]
  // "options_ui": {
